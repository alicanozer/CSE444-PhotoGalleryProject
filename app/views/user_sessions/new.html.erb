<h1>Giriş Yap</h1>

<%= form_for @user_session, :url => authenticate_path do |f| %>
    <%= f.error_messages %>
    <%= f.label :email %><br/>
    <%= f.text_field :email %><br/>
    <br/>
    <%= f.label :'Parola' %><br/>
    <%= f.password_field :password %><br/>
    <br/>
    <%= f.check_box :'Beni Hatırla' %><%= f.label :remember_me %><br/>
    <br/>
    <%= f.submit "Giriş Yap" %>

<% end %>